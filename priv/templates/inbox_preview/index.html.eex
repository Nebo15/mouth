      html, body {
        height: 100%
      }

      body {
        color: #333;
      }

      .list {
        border-right: 1px solid #ddd;
        padding-right: 0;
        padding-left: 0;
        overflow-x: hidden;
        overflow-y: auto;
        position: fixed;
        top: 0;
        bottom: 0;
        left: 0;
        z-index: 1000;
        display: block;
      }

      .list-group-item:first-child {
        border-top-left-radius: 0;
        border-top-right-radius: 0;
      }

      .list-group-item:last-child {
        border-bottom-right-radius: 0;
        border-bottom-left-radius: 0;
      }

      .list-group-item {
        border-right: none;
      }

      .content {
        padding-right: 0;
        padding-left: 0;
      }

      .header {
        border-bottom: 1px solid #ddd;
      }

      .header-content {
        padding: 1rem 2rem;
      }

      .headers {
        margin-left: 0;
        margin-bottom: 0;
      }

      .headers dt {
        padding-left: 0;
      }

      .headers dd {
        margin-bottom: 0;
      }

      .body {
        padding: 1rem 2rem;
      }

      .body-text {
        padding: 1.5rem;
        background-color: #f7f7f9;
      }

      .body-html {
        width: 100%;
        height: 100%;
        border: 2px solid #f7f7f9;
      }
    </style>
  </head>
  <body>
    <div class="container-fluid">
      <div class="row">
        <div class="col-sm-3 list">
          <div class="list-group">
            <%= if Enum.empty?(@messages) do %>
              <div class="list-group-item">
                <h6 class="list-group-item-heading">Empty inbox...</h6>
              </div>
            <% else %>
              <%= for message <- @messages do %>
                <% id = message.meta[:id] %>
                <a href="<%= to_absolute_url(@conn, id) %>" class="list-group-item<%= if @message && @message.meta[:id] == id, do: " active" %>">
                  <h6 class="list-group-item-heading">To: <%= message.to %></h6>
                </a>
              <% end %>
            <% end %>
          </div>
        </div>

        <div class="col-sm-offset-3 col-sm-9 content">
          <%= if @message do %>
            <div class="header">
              <div class="header-content">
                <dl class="dl-horizontal headers">
                  <dt class="col-sm-2">To</dt>
                  <dd class="col-sm-10"><%= @message.to %></dd>
                </dl>
              </div>
            </div>

            <div class="body">
              <%= if @message.body do %>
                <h5>Body:</h3>
                <pre class="body-text"><%= @message.body %></pre>
              <% end %>
            </div>
          <% else %>
            <div class="m-t-3 text-sm-center">
              <h3>Select a message to preview it</h3>
            </div>
          <% end %>
        </div>

      </div>
    </div>

    </div>
  </body>
</html>